{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base"],
  "plugins": ["jest"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "linebreak-style": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "global-require": 0,
    "radix": 0,
    "operator-linebreak": 0
  }
}
